{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "menu_categories",
  "info": {
    "name": "menu_category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "page_order": {
      "type": "integer"
    },
    "restaurant": {
      "model": "restaurant",
      "via": "menu_categories"
    },
    "food_items": {
      "collection": "food-item",
      "via": "menu_categories",
      "dominant": true
    },
    "menu_deals": {
      "collection": "menu-deal",
      "via": "menu_categories",
      "dominant": true
    }
  }
}
